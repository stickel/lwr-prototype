{
    "name": "core-prototype",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "types": "build/index.d.ts",
    "type": "module",
    "main": "build/index.js",
    "files": [
        "build/**/*.js"
    ],
    "scripts": {
        "clean": "rimraf build/ node_modules/ __lwr_cache__ tsconfig.tsbuildinfo yarn.lock",
        "lint": "eslint '**/*.{js,ts}' --max-warnings 0 -c ./config/.eslintrc.js --ignore-path ./.eslintignore",
        "format": "prettier --write '{modules,config,doc}/**/*.{js,ts,json,md}' --config ./.prettierrc --ignore-path ./.prettierignore",
        "prettier": "prettier --check '{modules,config,doc}/**/*.{js,ts,json,md}' --config ./.prettierrc --ignore-path ./.prettierignore",
        "build": "tsc -b && node ./scripts/copy-resources.mjs",
        "rebuild": "rimraf build/ __lwr_cache__ tsconfig.tsbuildinfo && yarn build",
        "dev": "lwr serve --port 3333",
        "start": "lwr serve --port 3000 --mode prod",
        "start:prod": "lwr serve --mode prod-compat"
    },
    "dependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-transform-modules-commonjs": "^7.15.4",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@lwrjs/label-module-provider": "^0.8.2",
        "@salesforce-ux/design-system": "^2.17.3",
        "@types/luxon": "2.0.9",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "classnames": "2.3.1",
        "core-js": "^3.18.1",
        "cpx2": "^4.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fs": "^0.0.1-security",
        "jest": "^27.1.1",
        "lightning-base-components": "^1.14.3-alpha",
        "luxon": "2.3.1",
        "lwc": "~2.3.7",
        "lwr": "0.8.2",
        "postcss": "^8.3.6",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.6"
    },
    "volta": {
        "node": "16.18.0",
        "yarn": "1.22.5"
    },
    "engines": {
        "node": "16.x"
    }
}
